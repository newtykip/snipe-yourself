{
    "name": "snipe-yourself",
    "description": "Generate a report of your osu! chokes to snipe them with ease!",
    "version": "1.0.0",
    "author": "newtykins",
    "bin": {
        "snipe": "./bin/run"
    },
    "bugs": "https://github.com/newtykins/snipe-yourself/issues",
    "dependencies": {
        "@oclif/core": "^1",
        "@oclif/plugin-autocomplete": "^1.0.0",
        "@oclif/plugin-commands": "^2.0.1",
        "@oclif/plugin-help": "^5",
        "bluebird": "^3.7.2",
        "chalk": "4.1.2",
        "conf": "^10.1.1",
        "console-table-printer": "^2.10.0",
        "dayjs": "^1.10.7",
        "figures": "3.2.0",
        "inquirer": "^8.2.0",
        "is-valid-path": "^0.1.1",
        "listr": "^0.14.3",
        "ord": "^1.0.1",
        "osu-api-extended": "2.5.30",
        "string-similarity": "^4.0.4",
        "terminal-link": "2.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.0.1",
        "@commitlint/config-conventional": "^16.0.0",
        "@types/bluebird": "^3.5.36",
        "@types/chai": "^4",
        "@types/inquirer": "^8.1.3",
        "@types/is-valid-path": "^0.1.0",
        "@types/listr": "^0.14.4",
        "@types/node": "^16.9.4",
        "@types/string-similarity": "^4.0.0",
        "chai": "^4",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^4",
        "eslint-config-oclif-typescript": "^1.0.2",
        "globby": "^11",
        "husky": "^7.0.4",
        "makensis": "^1.1.1",
        "oclif": "^2.0.0-main.10",
        "shx": "^0.3.3",
        "ts-node": "^10.2.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    },
    "dirname": "oex",
    "engines": {
        "node": ">=12.0.0"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/newtykins/snipe-yourself",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "oclif": {
        "bin": "snipe",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-autocomplete",
            "@oclif/plugin-commands"
        ],
        "topicSeparator": " ",
        "topics": {
            "config": {
                "description": "list and modify the config!"
            }
        }
    },
    "repository": "newtykins/snipe-yourself",
    "scripts": {
        "build": "shx rm -rf dist && tsc -b",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "pnpm lint",
        "prepack": "pnpm build && oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md",
        "prepare": "husky install",
        "commit": "cz"
    },
    "types": "dist/index.d.ts",
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "packageManager": "yarn@1.22.17"
}
